#TIME COMPLEXITY: O(N)
#SPACE COMPLEXITY: O(1)
def Find_duplicate_in_an_array(arr):

    for i in range(0,len(arr)):

        if(arr[abs(arr[i])]<0):
            print("Duplicate number is {0}".format(abs(arr[i])))
            

        else:

            arr[abs(arr[i])]=-arr[abs(arr[i])]


arr=[1,3,2,5,4,1,3]
Find_duplicate_in_an_array(arr)            

    

