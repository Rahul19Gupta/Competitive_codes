import pdb;pdb.set_trace()
s="AAABBB"

string=[]
count=0
for i in s:
    string.append(i)
length=len(string)
j=0
for i in range(0,length-1):
    print(length)
    print(string[j])
    print(string[j+1])
    if(string[j]==string[j+1]):
        string.remove(string[j])
        count=count+1
    else:
        j=j+1

print(count)




